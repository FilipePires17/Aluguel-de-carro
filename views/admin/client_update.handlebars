{{#data}}
<form name="cadastro" action="/atualizar" method="POST">
    <label>Nome de Usuario </label>
    <input type="text" name="user" id="user" placeholder="Usuário" value="{{CodCli}}"/><br/>
    <label>Nome Completo </label>
    <input type="text" name="name" id="name" placeholder="Nome" value="{{Nome}}"/><br/>
    <label>CNH </label>
    <input type="text" name="cnh" id="cnh" placeholder="CNH" value="{{CNH}}"/><br/>
    <label>Endereco </label>
    <input type="text" name="adress" id="adress" placeholder="Endereço" value="{{Endereco}}"/><br/>
    <input type="submit" name="signup" id="signup" value="Atualizar"/><br/>
</form>
{{/data}}